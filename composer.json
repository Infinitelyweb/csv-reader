{
	"name" : "saswo/csv-reader",
	"license" : "MIT",
	"version" : "0.0.1",
	"description" : "Read CSV files and map to typed PHP models.",
	"type" : "library",
	"authors" : [{
			"name" : "Sarah Swoboda",
			"email" : "infinitelyweb@gmail.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Saswo\\CsvReader\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Saswo\\CsvReader\\Tests\\" : "tests/"
		}
	},
	"require" : {
		"php" : ">=8.1",
		"league/csv" : "^9.8"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5",
		"phpstan/phpstan" : "^0.12",
		"fzaninotto/faker": "^1.5"
	},
	"suggest" : {
		"ext-intl" : "Required to use IntlValueConverter for advanced value conversion."
	},
	"scripts" : {
		"phpstan" : "vendor/bin/phpstan analyse src"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.x-dev"
		}
	}
}
